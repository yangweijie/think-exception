{
    "name": "yangweijie/think-exception",
    "description": "一个 ThinkPHP 中 可以打开错误异常文件的异常处理",
    "license": "MIT",
    "keywords": [
        "framework",
        "thinkphp",
        "exception"
    ],
    "require": {
        "php": ">=7.4",
        "topthink/framework": "^6.0 | ^6.1 | ^8.0",
        "yangweijie/think-editor": "*"
    },

    "authors": [
        {
            "name": "yangweijie",
            "email": "917647288@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "yangweijie\\exception\\": "src"
        }
    },
    "support": {
        "email": "917647288@qq.com"
    },
    "extra": {
        "think": {
            "services": [
                "yangweijie\\exception\\ExceptionService"
            ]
        }
    },
    "scripts":{
        "post-autoload-dump": [

        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
